@model IEnumerable<WebApplication1.Models.GuestResponse>

@{
    Layout = null;
}

<html xmlns="http://www.w3.org/1999/xhtml">
    <head><title>Responses</title>
        <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
        </head>
    <body>
        <div class="card-body">
        <h2>List of the people attending the meeting</h2>
        <table class="table table-striped table-bordered">
        <thead>
            <tr class="table-primary">
                <th>Name</th>
                <th>Phone</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (WebApplication1.Models.GuestResponse r in Model){
                <tr>
                <td>@r.Name</td>
                    <td>@r.Phone</td>
                        <td>@r.Email</td>
                            </tr>
            }
        </tbody>
        </table>
        </div>
        </body>
</html>